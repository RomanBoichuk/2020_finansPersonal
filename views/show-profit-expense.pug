extends loyout
block content
  h2 Доходи за період 
  h3 #{date}
  ul
    each val in profit
      li
        span= val.income 
        | 
        span=val.sum + " " + "грн."
  ul
    each val, id in expense
      li
        span= val.cost 
        | 
        span=val.prise + " " + "грн."
  h3 Баланс за день становить #{balansResult}   
  p
    a(href="/") ГОЛОВНА
  p
    a(href="calendar") Показати ДОХІД/ВИДАТОК
   
